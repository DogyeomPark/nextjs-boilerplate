{
  "name": "Nextjs-Boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "local:start": "NEXT_PUBLIC_ENV=local next dev",
    "local:build": "NEXT_PUBLIC_ENV=local next build",
    "dev:start": "NEXT_PUBLIC_ENV=development next dev",
    "dev:build": "NEXT_PUBLIC_ENV=development next build && next-sitemap",
    "prod:start": "NEXT_PUBLIC_ENV=production next start",
    "prod:build": "NEXT_PUBLIC_ENV=production next build && next-sitemap",
    "format": "npx prettier --write \"app/**/*.ts\"",
    "generate-api": "dotenv -e .env.local -- bash -c  'swagger-typescript-api --extract-request-params -p $SWAGGER_URL --module-name-index 0 -o src/generated/api -n api-service'"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@types/node": "18.16.2",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.1",
    "dotenv-cli": "^7.3.0",
    "eslint": "8.39.0",
    "eslint-config-next": "13.3.1",
    "next": "14.1.0",
    "next-auth": "5.0.0-beta.5",
    "next-sitemap": "^4.2.3",
    "react": "18.2.0",
    "react-cookie": "^7.0.2",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.9",
    "react-hot-toast": "^2.4.1",
    "swagger-typescript-api": "^13.0.3",
    "typescript": "5.0.4",
    "zod": "^3.22.4",
    "zustand": "^4.3.7"
  }
}
