{
  "name": "Nextjs-Boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next-sitemap",
    "start": "next start",
    "lint": "next lint",
    "local:start": "i18nexus pull && NEXT_PUBLIC_ENV=local next dev",
    "local:build": "i18nexus pull && NEXT_PUBLIC_ENV=local next build",
    "dev:start": "i18nexus pull && NEXT_PUBLIC_ENV=development next dev",
    "dev:build": "i18nexus pull && NEXT_PUBLIC_ENV=development next build",
    "format": "npx prettier --write \"app/**/*.ts\"",
    "generate-api": "dotenv -e .env.local -- bash -c  'swagger-typescript-api --extract-request-params -p $SWAGGER_URL --module-name-index 0 -o src/generated/api -n api-service'"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@tanstack/react-query": "^5.17.19",
    "@tanstack/react-query-devtools": "^5.17.21",
    "@tanstack/react-query-next-experimental": "^5.17.19",
    "@types/node": "18.16.2",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.1",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "clsx": "^2.1.0",
    "date-fns": "^2.30.0",
    "dotenv-cli": "^7.3.0",
    "eslint": "8.39.0",
    "eslint-config-next": "13.3.1",
    "i18next": "^23.8.2",
    "i18next-resources-to-backend": "^1.2.0",
    "leaflet": "^1.9.3",
    "next": "14.1.0",
    "next-auth": "5.0.0-beta.5",
    "next-cloudinary": "5.20.0",
    "next-i18n-router": "^5.2.1",
    "next-sitemap": "^4.2.3",
    "query-string": "^8.1.0",
    "react": "18.2.0",
    "react-date-range": "^1.4.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.9",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^14.0.5",
    "react-icons": "^4.8.0",
    "react-leaflet": "^4.2.1",
    "react-select": "^5.7.3",
    "react-spinners": "^0.13.8",
    "swagger-typescript-api": "^13.0.3",
    "typescript": "5.0.4",
    "world-countries": "^4.0.0",
    "zod": "^3.22.4",
    "zustand": "^4.3.7"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/leaflet": "^1.9.3",
    "@types/react-date-range": "^1.4.4",
    "i18nexus-cli": "^3.3.0"
  }
}
